  "peerDependencies": {
    "tailwindcss": ">=3.0.0 || >= 3.0.0-alpha.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "chokidar-cli": "^2.1.0",
    "clean-css": "^4.2.1",
    "concurrently": "^5.3.0",
    "live-server": "^1.2.1",
    "postcss": "^8.1.7",
    "tailwindcss": "next"
  }
}

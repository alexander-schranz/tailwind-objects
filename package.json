{
  "name": "tailwind-objects",
  "version": "0.1.0-alpha.1",
  "description": "Style objects for tailwind.",
  "main": "src/index.js",
  "repository": "https://github.com/alexander-schranz/tailwind-objects",
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run watch\"",
